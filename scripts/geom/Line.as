package geom
{
   public class Line
   {
       
      
      public var x1:Number;
      
      public var x2:Number;
      
      public var y1:Number;
      
      public var y2:Number;
      
      public function Line(x1:Number, y1:Number, x2:Number, y2:Number)
      {
         super();
         this.x1 = x1;
         this.x2 = x2;
         this.y1 = y1;
         this.y2 = y2;
      }
   }
}
